{
  "name": "lgtm-cat-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2222",
    "build": "next build",
    "start": "next start -p 2222",
    "lint:prettier": "prettier --check --log-level=warn '**/!(*.min|*-lock).{yaml,yml,md,mdx}'",
    "prettier": "prettier --write --log-level=warn '**/!(*.min|*-lock).{yaml,yml,md,mdx}'",
    "lint": "ultracite check && npm run lint:prettier",
    "format": "ultracite fix && npm run prettier",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "sync:agents-docs": "tsx src/scripts/copy-agents-markdown.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.958.0",
    "@aws-sdk/s3-request-presigner": "3.958.0",
    "@heroui/react": "2.8.7",
    "@next/third-parties": "16.1.1",
    "@sentry/nextjs": "10.32.1",
    "@upstash/ratelimit": "2.0.7",
    "@upstash/redis": "1.36.0",
    "@vercel/edge-config": "1.4.3",
    "@vercel/functions": "3.3.4",
    "framer-motion": "12.23.26",
    "next": "16.1.1",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-aria-components": "1.14.0",
    "react-dom": "19.2.3",
    "react-markdown": "10.1.0",
    "recharts": "3.6.0",
    "usehooks-ts": "3.1.1",
    "zod": "4.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@chromatic-com/storybook": "4.1.3",
    "@iconify/react": "6.0.2",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-onboarding": "10.1.11",
    "@storybook/addon-viewport": "9.0.8",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/nextjs-vite": "10.1.11",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@vitest/browser-playwright": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "ajv": "8.17.1",
    "babel-plugin-react-compiler": "1.0.0",
    "chromatic": "13.3.4",
    "globals": "16.5.0",
    "jsdom": "27.4.0",
    "msw": "2.12.7",
    "msw-storybook-addon": "2.0.6",
    "npm-run-all": "4.1.5",
    "playwright": "1.57.0",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "storybook": "10.1.11",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "ultracite": "6.5.1",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  },
  "msw": {
    "workerDirectory": "public"
  }
}

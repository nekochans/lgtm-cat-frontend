{
  "name": "lgtm-cat-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2222",
    "build": "next build",
    "start": "next start -p 2222",
    "lint:prettier": "prettier --cache --check .",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint:eslint": "eslint --cache .",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "lint": "run-s lint:*",
    "format": "run-s fix:*",
    "test": "vitest --exclude **/tests-e2e/**",
    "test:coverage": "vitest run --coverage --exclude **/tests-e2e/**",
    "test:ci": "vitest run --coverage --exclude **/tests-e2e/**",
    "test:e2e": "npx playwright test",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:codegen": "npx playwright codegen",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@sentry/nextjs": "8.26.0",
    "@upstash/ratelimit": "^2.0.1",
    "@upstash/redis": "^1.34.0",
    "@vercel/edge-config": "^1.2.1",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-aria-components": "^1.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@chromatic-com/storybook": "^1.6.1",
    "@eslint-react/eslint-plugin": "^1.14.3",
    "@next/env": "^14.2.5",
    "@playwright/test": "^1.47.2",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/nextjs": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.7.5",
    "@types/prettier": "^2.7.3",
    "@types/react": "18.3.3",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "ajv": "^8.17.1",
    "chromatic": "^11.7.1",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "globals": "^15.9.0",
    "jsdom": "^24.1.1",
    "msw": "^2.3.5",
    "msw-storybook-addon": "2.0.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.47.2",
    "postcss": "^8.4.41",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.10",
    "typescript": "5.5.4",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}

{
  "name": "lgtm-cat-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2222",
    "build": "next build",
    "start": "next start -p 2222",
    "lint:prettier": "prettier --check --log-level=warn '**/!(*.min|*-lock).{yaml,yml,md,mdx}'",
    "prettier": "prettier --write --log-level=warn '**/!(*.min|*-lock).{yaml,yml,md,mdx}'",
    "lint": "ultracite check && npm run lint:prettier",
    "format": "ultracite fix && npm run prettier",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "sync:agents-docs": "tsx src/scripts/copy-agents-markdown.ts"
  },
  "dependencies": {
    "@heroui/react": "2.8.5",
    "@sentry/nextjs": "10.25.0",
    "@upstash/ratelimit": "2.0.7",
    "@upstash/redis": "1.35.6",
    "@vercel/edge-config": "1.4.3",
    "@vercel/functions": "3.3.2",
    "framer-motion": "12.23.24",
    "next": "16.0.3",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-aria-components": "1.13.0",
    "react-dom": "19.2.0",
    "recharts": "3.4.1",
    "usehooks-ts": "3.1.1",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@chromatic-com/storybook": "4.1.3",
    "@iconify/react": "6.0.2",
    "@storybook/addon-a11y": "10.0.8",
    "@storybook/addon-docs": "10.0.8",
    "@storybook/addon-onboarding": "10.0.8",
    "@storybook/addon-viewport": "9.0.8",
    "@storybook/addon-vitest": "10.0.8",
    "@storybook/nextjs-vite": "^10.0.8",
    "@tailwindcss/postcss": "4.1.17",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.6",
    "@vitest/browser-playwright": "4.0.10",
    "@vitest/coverage-v8": "4.0.10",
    "ajv": "8.17.1",
    "babel-plugin-react-compiler": "1.0.0",
    "chromatic": "13.3.4",
    "globals": "16.5.0",
    "jsdom": "27.2.0",
    "msw": "2.12.2",
    "msw-storybook-addon": "2.0.6",
    "npm-run-all": "4.1.5",
    "playwright": "1.56.1",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "storybook": "10.0.8",
    "tailwindcss": "4.1.17",
    "tsx": "4.20.6",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "ultracite": "6.3.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.10"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
